<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<fx:root type="javafx.scene.layout.BorderPane"
         xmlns:fx="http://javafx.com/fxml"
         styleClass="toolboxItem">
    <top>
        <HBox style="-fx-background-color: slategray;">
            <children>
                <Region prefHeight="10" prefWidth="20"/>
                <Region prefHeight="10" prefWidth="20"
                        style="-fx-background-color: white;"/>
            </children>
        </HBox>
    </top>
    <center>
        <HBox prefHeight="100.0" prefWidth="200.0"
              style="-fx-background-color: slategray;">
            <Label fx:id="nameLabel"
                    style="-fx-padding: 0 0 0 10"/>
        </HBox>
    </center>
    <bottom>
        <HBox style="-fx-background-color: transparent;">
            <children>
                <Region prefHeight="10" prefWidth="20"/>
                <Region prefHeight="10" prefWidth="20"
                        style="-fx-background-color: slategray;"/>
            </children>
        </HBox>
    </bottom>
    <center>

    </center>
</fx:root>