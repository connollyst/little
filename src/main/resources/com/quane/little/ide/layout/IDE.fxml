<?xml version="1.0" encoding="UTF-8"?>
<?import com.quane.little.ide.controls.MenuBar?>
<?import com.quane.little.ide.layout.Toolbox?>
<?import com.quane.little.ide.layout.Workspace?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.BorderPane?>
<?import java.net.URL?>
<fx:root type="javafx.scene.Group" xmlns:fx="http://javafx.com/fxml">
    <BorderPane id="root"
                maxHeight="-Infinity" maxWidth="-Infinity"
                minHeight="-Infinity" minWidth="-Infinity"
                prefHeight="600.0" prefWidth="800.0"
                xmlns:fx="http://javafx.com/fxml">
        <top>
            <MenuBar fx:id="menubar"/>
        </top>
        <center>
            <Workspace fx:id="workspace" prefHeight="200.0" prefWidth="200.0"/>
        </center>
        <left>
            <Toolbox fx:id="toolbox" alignment="TOP_CENTER" prefHeight="400.0"
                     prefWidth="150.0" spacing="10.0">
                <padding>
                    <Insets bottom="20.0" left="10.0" right="10.0" top="20.0"/>
                </padding>
            </Toolbox>
        </left>
        <stylesheets>
            <URL value="@IDE.css"/>
        </stylesheets>
    </BorderPane>
</fx:root>
